    document.execCommand(command, false);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAQA,OAAOA,UAAP,MAAuB,8BAAvB;AAEA,eAAe;AACXC,MAAI,EAAE,aADK;AAGXC,YAAU,EAAC;AACPF;AADO,GAHA;;AAOXG,MAAI,GAAE;AACF,WAAM;AACFC,YAAM,EAAE;AADN,KAAN;AAGH,GAXU;;AAaXC,SAAO,EAAE;AACLC,YAAQ,CAACC,CAAD,EAAG;AACP,UAAI,KAAKH,MAAL,IAAe,IAAnB,EAAwB;AACpB,YAAIG,CAAC,CAACC,OAAF,IAAa,EAAjB,EAAoB;AAChBD,WAAC,CAACE,cAAF;AACAC,eAAK,CAAC,aAAD,CAAL;AACA,eAAKN,MAAL,GAAc,KAAd;AACJ,SAJA,MAMK,IAAIG,CAAC,CAACC,OAAF,IAAa,EAAjB,EAAoB;AACzBD,WAAC,CAACE,cAAF;AACA,gBAAME,SAAQ,GAAIC,MAAM,CAACC,YAAP,EAAlB;AACA,gBAAMC,KAAI,GAAIC,QAAQ,CAACC,WAAT,EAAd;AACAL,mBAAS,CAACM,eAAV;AACAH,eAAK,CAACI,kBAAN,CAAyBX,CAAC,CAACY,MAA3B;AACAL,eAAK,CAACM,QAAN,CAAe,KAAf;AACAT,mBAAS,CAACU,QAAV,CAAmBP,KAAnB;AACAP,WAAC,CAACY,MAAF,CAASG,KAAT;AACA,SATK,MAUA,IAAIf,CAAC,CAACC,OAAF,IAAa,EAAjB,EAAoB;AACzBD,WAAC,CAACE,cAAF;AACA,eAAKc,SAAL,CAAeX,MAAf,EAAuB,CAAC,CAAxB;AACA,SAHK,MAIA,IAAIL,CAAC,CAACC,OAAF,IAAa,EAAjB,EAAoB;AACzBD,WAAC,CAACE,cAAF;AACAF,WAAC,CAACY,MAAF,CAASK,iBAAT,CAA2BjB,CAAC,CAACY,MAAF,CAASM,cAAT,GAAwB,CAAnD,EAAsDlB,CAAC,CAACY,MAAF,CAASM,cAAT,GAAwB,CAA9E;AACA,SAHK,MAKA,IAAIlB,CAAC,CAACC,OAAF,IAAa,EAAjB,EAAoB,CAAC,CAArB,MACA,IAAID,CAAC,CAACC,OAAF,IAAa,EAAjB,EAAoB,CAAC,CAArB,MACA,IAAID,CAAC,CAACC,OAAF,IAAa,EAAjB,EAAoB,CAAC,CAArB,MACA,IAAID,CAAC,CAACC,OAAF,IAAa,EAAjB,EAAoB,CAAC,CAArB,MAEA;AACDD,WAAC,CAACE,cAAF;AACJ;AACJ,OAlCA,MAmCK,IAAI,KAAKL,MAAL,IAAe,KAAnB,EAAyB;AAC1B,YAAIG,CAAC,CAACC,OAAF,IAAa,EAAjB,EAAoB;AAChBE,eAAK,CAAC,aAAD,CAAL;AACA,eAAKN,MAAL,GAAc,IAAd;AACJ;AACJ;AACH,KA3CI;;AA6CTmB,aAAS,CAACG,GAAD,EAAMC,SAAN,EAAiB;AAC1B,UAAIC,GAAJ,EAASd,KAAT;;AACA,UAAIY,GAAG,CAACb,YAAR,EAAsB;AAClBe,WAAE,GAAIF,GAAG,CAACb,YAAJ,EAAN;;AACA,YAAIe,GAAG,CAACC,UAAJ,GAAiB,CAArB,EAAwB;AACpB,cAAIC,QAAO,GAAIF,GAAG,CAACG,SAAnB;AACA,cAAIC,SAAQ,GAAIJ,GAAG,CAACK,WAAJ,GAAkBN,SAAlC;AACAC,aAAG,CAACR,QAAJ,CAAaU,QAAb,EAAuBI,IAAI,CAACC,GAAL,CAASL,QAAQ,CAACM,MAAlB,EAA0BJ,SAA1B,CAAvB;AACJ;AACJ,OAPA,MAOO,IAAMJ,GAAE,GAAIF,GAAG,CAACX,QAAJ,CAAaJ,SAAzB,EAAsC;AACzC,Y